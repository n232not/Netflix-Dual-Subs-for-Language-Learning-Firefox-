 {
  "manifest_version": 2,
  "name": "Subadub",
  "description" : "Enhanced Netflix subtitles for foreign language study",
  "version": "0.1.9",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.netflix.com/*"],
      "run_at": "document_end",
      "js": ["content_script.js"]
    }
  ],
  "permissions": [
    "*://*.githubusercontent.com/*"
  ]
 }
